<Window x:Class="TsGui.View.Layout.ConsoleOutput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TsGui.View.Layout"
        mc:Ignorable="d"
        Title="TsGui Output" Height="350" Width="800">
    <Grid Background="LightGray">
        <TextBox Background="Transparent" BorderThickness="0" Text="{Binding Text, Mode=OneWay}" IsReadOnly="True" TextWrapping="Wrap" Foreground="Black" Margin="5" />
    </Grid>
</Window>

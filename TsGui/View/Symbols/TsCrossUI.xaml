<UserControl x:Class="TsGui.View.Symbols.TsCrossUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:local="clr-namespace:TsGui.View.Symbols"
        mc:Ignorable="d" 
        d:DesignHeight="20" d:DesignWidth="20">
    <Grid Style="{StaticResource ControlStyle}">
        <Path Name="CrossMark" Stroke="OrangeRed" StrokeThickness="0.5" StrokeEndLineCap="Square" Fill="Red" Data="M0,3 8,10 0,17 3,20 10,12 17,20 20,17 12,10 20,3 17,0 10,8 3,0 0,3" Visibility="Visible" Stretch="Uniform" />
    </Grid>
</UserControl>

<UserControl x:Class="TsGui.View.GuiOptions.TsTickUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:local="clr-namespace:TsGui.View.GuiOptions"
        mc:Ignorable="d" 
        d:DesignHeight="20" d:DesignWidth="20">

    <Grid Style="{StaticResource ControlStyle}">
        <Path Name="TickMark" Stroke="DarkGreen" StrokeThickness="0.5" Fill="Green" Data="M0,10 8,20 20,0 7,14.5 0,10" Visibility="Visible" Stretch="Uniform" />
    </Grid>
</UserControl>

<UserControl x:Class="TsGui.View.GuiOptions.TsCrossUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:local="clr-namespace:TsGui.View.GuiOptions"
        mc:Ignorable="d" 
        d:DesignHeight="20" d:DesignWidth="20">
    <StackPanel Style="{StaticResource ControlStyle}">
        <Path Name="CrossMark" Stroke="OrangeRed" StrokeThickness="0.5" StrokeEndLineCap="Square" Fill="Red" Data="M0,4 3,10 0,16 2,20 5,14 8,20 10,16 7,10 10,4 8,0 5,6 2,0 0,4 " Visibility="Visible" Stretch="Fill" />
    </StackPanel>
</UserControl>

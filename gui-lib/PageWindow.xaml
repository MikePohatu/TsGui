<Window x:Class="gui_lib.PageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gui_lib"
        mc:Ignorable="d"
        Title="PageWindow" Height="300" Width="300">
    <Grid x:Name="MainGrid">
        <Grid.RowDefinitions>
            <RowDefinition x:Name ="MainRow" Height="Auto"/>
            <RowDefinition x:Name="ButtonRow" Height="30*" />
        </Grid.RowDefinitions>
        <Button x:Name="buttonNext" Content="Next" HorizontalAlignment="Left" Margin="210,242,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="buttonPrev" Content="Back" HorizontalAlignment="Left" Margin="130,242,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="10,242,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
